{
  "name": "replicad",
  "version": "0.12.1",
  "description": "The library to build browser based 3D models with code",
  "keywords": [
    "cad",
    "opencascadejs",
    "brep",
    "STEP",
    "STL"
  ],
  "author": "Steve Genoud <steve@sgenoud.com>",
  "homepage": "https://replicad.xyz",
  "license": "AGPL-3.0-or-later",
  "main": "dist/umd/replicad.js",
  "module": "dist/es/replicad.js",
  "directories": {
    "src": "src",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts",
    "start": "rollup -w -c rollup.config.ts",
    "build-doc": "typedoc --exclude \"../replicad-opencascadejs/*\" --out docs --theme minimal src",
    "build-docss": "typedoc --help",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.1.0",
    "prettier": "^2.4.1",
    "replicad-opencascadejs": "^0.12.0",
    "rollup": "^2.58.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^1.4.7",
    "typedoc": "^0.22.7",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@types/opentype.js": "^1.3.3",
    "opentype.js": "^1.3.4"
  }
}
